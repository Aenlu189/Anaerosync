                  <HBox alignment="CENTER" spacing="20">
                     <children>
                        <Button fx:id="viewCompletedTasksButton" style="-fx-font-size: 12px;" text="View Completed Tasks" />
                        <Button fx:id="viewOwnedTasksButton" style="-fx-font-size: 12px;" text="View Owned Tasks" />
                        <Button fx:id="OkButton" onAction="#OkViewTask" style="-fx-font-size: 14px;" text="Close" />
                     </children>
                     <VBox.margin>
                        <Insets top="20.0" />
                     </VBox.margin>
                  </HBox>
               </children>
            </VBox>
         </children>
      </AnchorPane>
   </children>
</HBox>
