                  </ScrollPane>
                  <HBox alignment="CENTER" spacing="20">
                     <children>
                        <Button fx:id="confirmCompleteButton" onAction="#confirmCompleteTask" style="-fx-font-size: 14px;" text="Complete Selected Task" />
                        <Button fx:id="cancelCompleteButton" onAction="#cancelCompleteTask" style="-fx-font-size: 14px;" text="Cancel" />
