                        <FlowPane fx:id="currentPlayerCards" hgap="10" vgap="10" />
                     </ScrollPane>
                  </VBox>
                  
                  <!-- Trade arrows -->
                  <VBox alignment="CENTER" prefHeight="916.0" prefWidth="194.0" spacing="10">
