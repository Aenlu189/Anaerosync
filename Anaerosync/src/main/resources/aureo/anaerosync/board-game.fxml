                        <Insets bottom="10.0" />
                     </VBox.margin>
                  </Label>
                  <HBox alignment="CENTER" spacing="10">
                     <children>
                        <Label style="-fx-font-size: 16px;" text="Task Count: " />
                        <Label fx:id="taskCountLabel" style="-fx-font-size: 16px; -fx-font-weight: bold;" text="0" />
                     </children>
                     <VBox.margin>
                        <Insets bottom="20.0" />
                     </VBox.margin>
                  </HBox>
                  <ScrollPane fitToWidth="true" prefHeight="600.0">
                     <content>
                        <FlowPane fx:id="taskList" hgap="10" vgap="10" />
