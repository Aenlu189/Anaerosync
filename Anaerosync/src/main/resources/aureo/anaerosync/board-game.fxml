            <!-- Add the offer modal -->
            <VBox fx:id="offerModal" alignment="CENTER" layoutX="2.0" prefHeight="958.0" prefWidth="1442.0" spacing="10" style="-fx-background-color: white; -fx-padding: 20; -fx-border-color: black; -fx-border-width: 1;" visible="false">
               <children>
                  <Label style="-fx-font-size: 16px;" text="Choose Player to Offer Task:" />
